{
  "name": "slashworks/contao-trackingmanager-bundle",
  "type": "contao-bundle",
  "description": "Contao Cookie Manager",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "slashworks",
      "homepage": "https://github.com/slashworks"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "4.4.*",
    "symfony/framework-bundle": "^3.3"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "friendsofphp/php-cs-fixer": "^2.12",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message-factory": "^1.0.2",
    "phpunit/phpunit": "^5.7.26",
    "symfony/phpunit-bridge": "^3.2"
  },
  "extra": {
    "contao-manager-plugin": "Slashworks\\ContaoTrackingManagerBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Contao\\SkeletonBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slashworks\\ContaoTrackingManagerBundle\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/slashworks/trackingmanager-bundle/issues",
    "source": "https://github.com/slashworks/trackingmanager-bundle"
  }
}